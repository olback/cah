{
  "name": "cah-server",
  "version": "1.0.0",
  "description": "CAH Node.js Server",
  "main": "out/server.js",
  "scripts": {
    "prestart": "npm install && npm run ts:compile",
    "start": "node out/server.js",
    "ts:compile": "node_modules/.bin/tsc -p ./",
    "ts:watch": "node_modules/.bin/tsc -p ./ --watch",
    "ts:lint": "node_modules/.bin/tslint --project .",
    "dev": "npm rebuild --build-from-source && node devserver",
    "pretest": "node_modules/.bin/tsc -p tsconfig.test.json",
    "test": "mocha out/test"
  },
  "keywords": [
    "cah",
    "game",
    "nodejs"
  ],
  "author": "olback",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "concurrently": "^4.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.9",
    "tslint": "^5.16.0"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/pg": "^7.4.14",
    "@types/socket.io": "^2.1.2",
    "express": "^4.18.2",
    "git-rev-sync": "^1.12.0",
    "pg": "^8.7.1",
    "socket.io": "^2.2.0",
    "solid-log": "^1.0.2",
    "typescript": "^3.2.2"
  }
}
